{
    "name": "xwines",
    "version": "1.0.0",
    "description": "XWines-Inventory Management System",
    "scripts": {
        "dev": "webpack serve --config webpack/webpack.config.js --env env=dev",
        "build": "webpack --config webpack/webpack.config.js --env env=prod",
        "start": "cd build && npx serve",
        "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
        "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
        "publish-package": "yarn install && yarn build && cd build && npm publish"
    },
    "repository": "git@github.com:xwines-in/xwines-dashboard.git",
    "keywords": [
        "React",
        "Typescript",
        "Webpack 5"
    ],
    "license": "ISC",
    "dependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.0",
        "@fontsource/roboto": "^5.0.8",
        "@mui/icons-material": "^5.15.10",
        "@mui/material": "^5.15.12",
        "react": "^18.2.0",
        "react-app-polyfill": "^3.0.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.22.1",
        "resize-observer-polyfill": "^1.5.1"
    },
    "devDependencies": {
        "@babel/core": "^7.23.9",
        "@babel/plugin-transform-runtime": "^7.23.9",
        "@babel/preset-env": "^7.23.9",
        "@babel/preset-react": "^7.23.3",
        "@babel/preset-typescript": "^7.23.3",
        "@babel/runtime": "^7.23.9",
        "@types/estree": "^1.0.5",
        "@types/react": "^18.2.56",
        "@types/react-dom": "^18.2.19",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^7.0.1",
        "@typescript-eslint/parser": "^7.0.1",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^6.10.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "html-webpack-plugin": "^5.6.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "sass": "^1.71.0",
        "sass-loader": "^14.1.0",
        "serve": "^14.2.1",
        "style-loader": "^3.3.4",
        "typescript": "^5.3.3",
        "webpack": "^5.90.2",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json}": [
            "eslint --fix"
        ],
        "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write"
        ]
    },
    "private": true
}
